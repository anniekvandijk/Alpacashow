@page
@model Alpacashow.Mvc.Pages.Showevents.IndexModel

@{
    ViewData["Title"] = "Index";
}

<h2>Index</h2>

<p>
    <a asp-page="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ShowEvent[0].Name)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ShowEvent[0].Date)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ShowEvent[0].Judge)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ShowEvent[0].Location)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.ShowEvent[0].Archived)
                </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model.ShowEvent) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Name)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Date)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Judge)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Location)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Archived)
            </td>
            <td>
                <a asp-page="./Edit" asp-route-id="@item.ShowEventId">Edit</a> |
                <a asp-page="./Details" asp-route-id="@item.ShowEventId">Details</a> |
                <a asp-page="./Delete" asp-route-id="@item.ShowEventId">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
